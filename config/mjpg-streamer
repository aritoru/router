config mjpg-streamer 'core'
	option input 'uvc'
	option output 'http'
	option device '/dev/video0'
	option www '/www/webcam'
	option enabled '0'
	option yuv '1'
	option no_dynctrl '1'
	option led 'auto'
	option port '1234'
	option enable_auth '1'
	option username 'webcam'
	option password 'superwrt'
	option resolution '640x480'
	option fps '1'
