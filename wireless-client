config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT40'
	option country 'RO'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	option txpower '0'
	option disabled '0'
	option channel '8'

config wifi-iface
	option network 'wwan'
	option encryption 'psk'
	option device 'radio0'
	option mode 'sta'
	option bssid '10:FE:ED:AC:48:48'
	option key '%%WIFI_CLIENT_PASS%%'
	option ssid '%%WIFI_CLIENT_SSID%%'
